<!DOCTYPE html>
<html>
<head>

    <title>Lecturing</title>
    <!--meta tags -->

    <script>
         addEventListener("load", function () {
         	setTimeout(hideURLbar, 0);
         }, false);

         function hideURLbar() {
         	window.scrollTo(0, 1);
         }
    </script>

    <!--//meta tags ends here-->
    <!--booststrap-->
    <link href="~/css/bootstrap.min.css" rel="stylesheet" type="text/css" media="all">
    <!--//booststrap end-->
    <!-- font-awesome icons -->
    <link href="~/css/font-awesome.min.css" rel="stylesheet">
    <!-- //font-awesome icons -->
    <!--stylesheets-->
    <link href="~/css/style.css" rel='stylesheet' type='text/css' media="all">
    <!--//stylesheets-->
    <link href="//fonts.googleapis.com/css?family=Fira+Sans:400,500,600,700" rel="stylesheet">
    <link href="//fonts.googleapis.com/css?family=Oxygen:400,700" rel="stylesheet">




</head>
<body>



    <div class="main-top" id="home">
        <!-- header -->
        <div class="headder-top">
            <!-- nav -->
            <nav>
                <div id="logo">
                    <h1><a href="index.html">Lecturing</a></h1>
                </div>
                <label for="drop" class="toggle">Menu</label>
                <input type="checkbox" id="drop">
                <ul class="menu mt-2">

                    <li class="active">@Html.ActionLink("Home", "Index", "Home")</li>
                    <li class="mx-lg-3 mx-md-2 my-md-0 my-1"> @Html.ActionLink("About", "About", "Home")</li>
                    <li class="mx-lg-3 mx-md-2 my-md-0 my-1"> @Html.ActionLink("Gallery", "Gallery", "Home")</li>

                    <li> @Html.ActionLink("Admin Login", "AdminLogin", "Home") </li>
                    
                    <li> @Html.ActionLink("Contact", "Contact", "Home") </li>
                </ul>
            </nav>
            <!-- //nav -->
        </div>
        <!-- //header -->
        <!-- banner -->
        <div class="main-banner text-center">
            <div class="container">
                <div class="style-banner ">
                    <h4 class="mb-lg-3 mb-2">Practice Makes You Better</h4>
                    <h5>
                        Its Time To Master Your Skills
                    </h5>
                </div>
                <div class="two-demo-button mt-lg-5 mt-md-4 mt-3">

                </div>
                <div class="view-buttn mt-md-4 mt-sm-4 mt-3">

                </div>
            </div>
        </div>
    </div>
    <!-- //banner -->


    <div class="container body-content">
        @RenderBody()
        <hr />
    </div>


    <!-- footer -->
    <footer class="py-lg-4 py-md-3 py-sm-3 py-3">
        <div class="container py-lg-5 py-md-5 py-sm-4 py-3">

            <div class="bottem-wthree-footer text-center pt-lg-5 pt-md-4 pt-3">
                <p>
                    © 2020 Lecturing. All Rights Reserved | Design by Lecturing
                </p>
            </div>
            <!-- move icon -->
            <div class="text-center">
                <a href="#home" class="move-top text-center mt-3"></a>
            </div>
            <!--//move icon -->
        </div>
    </footer>
    <!--//footer -->

</body>
</html>
